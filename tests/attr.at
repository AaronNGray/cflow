AT_SETUP([attribute handling])
AT_KEYWORDS([attr])

CFLOW_OPT([-v],[

# Early versions of cflow coredumped on this output
CFLOW_CHECK([
extern main() __attribute__((noreturn));

main()
{
     foo();
}
],
[main() <main () at prog:4>:
    foo()],
[prog:2: expected `;' near `__attribute__'
])

])

AT_CLEANUP



