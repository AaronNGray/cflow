AT_SETUP([parameter wrapper handling])
AT_KEYWORDS([pwrapper])

CFLOW_OPT([--symbol PARM:wrapper],[

CFLOW_CHECK([
int foo PARM((int x, int y));

int
foo(int x, int y)
{
     bar();
}
],
[foo() <int foo (int x, int y) at prog:5>:
    bar()])

])

AT_CLEANUP



